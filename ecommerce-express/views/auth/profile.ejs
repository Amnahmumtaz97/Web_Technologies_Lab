<section class="auth-section">
  <h2>User Profile</h2>
  <div class="profile-info">
    <p><strong>Username:</strong> <%= user.username %></p>
    <p><strong>Email:</strong> <%= user.email %></p>
    <p><strong>Role:</strong> <%= user.role %></p>
    <a href="/logout" class="btn">Logout</a>
  </div>
</section>
